@model NbSites.Web.Controllers.ImagesViewModel
@{
    var groups = Model.Images.GroupBy(x => x.Category).ToList();
    ViewBag.Title = "Images";
}
<h2>Image Gallery</h2>
@foreach (var group in groups)
{
    <h2>@group.Key</h2>
    <div class="row">
        @foreach (var item in group)
        {
            <div class="col-xs-6 col-md-3">
                <div class="thumbnail">
                    <img src="@item.Url" alt="@item.Title" title="@item.Title"/>
                    <div class="caption text-center">
                        <h3>@item.Title</h3>
                        <p>owner: @item.OwnerId</p>
                    </div>
                </div>
            </div>
        }
    </div>
}